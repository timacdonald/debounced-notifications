{
    "name": "timacdonald/notification-throttle",
    "authors": [
        {
            "name": "Tim MacDonald",
            "email": "hello@timacdonald.me"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "TiMacDonald\\ThrottledNotifications\\": "src/"
        },
        "classmap": [
            "migrations"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "classmap": [
            "tests/migrations"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "TiMacDonald\\ThrottledNotifications\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpstan/phpstan": "^0.12",
        "vimeo/psalm": "^3.0"
    },
    "scripts": {
        "lint": [
            "clear",
            "./vendor/bin/php-cs-fixer fix",
            "./vendor/bin/psalm",
            "./vendor/bin/phpstan analyse"
        ],
        "test": [
            "clear",
            "./vendor/bin/phpunit"
        ]
    }
}
